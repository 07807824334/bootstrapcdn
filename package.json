{
  "name": "bootstrapcdn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "run": "node app.js",
    "setup": "npm install",
    "start": "NODE_ENV=production forever -m 4 -p logs -l server.log --append --plain start server.js",
    "status": "forever list",
    "stop": "forever stop server.js",
    "test": "mocha --timeout 15000 ./tests/*_test.js ./tests/**/*_test.js -R spec",
    "test-nc": "mocha --no-colors --timeout 15000 ./tests/*_test.js ./tests/**/*_test.js -R spec"
  },
  "dependencies": {
    "comma-it": "~0.0.5",
    "dateformat": "~1.0.7-1.2.3",
    "forever": "~0.10.11",
    "maxcdn": "^0.1.4",
    "jade": "~1.1.5",
    "js-yaml": "~3.0.1",
    "morgan": "~1.0.1",
    "express": "~4.2.0",
    "serve-favicon": "~2.0.0",
    "serve-static": "~1.1.0",
    "errorhandler": "~1.0.1"
  },
  "devDependencies": {
    "expect.js": "~0.2.0",
    "format": "~0.2.1",
    "mocha": "~1.17.1",
    "net-ping": "^1.1.11",
    "superagent": "~0.16.0",
    "uglify-js": "^2.4.15"
  }
}
