extends layout

block content
    h2 Showcase
    .well.text-center
        - each item in config.showcase
            .row
                .col-md-12.text-center
                    br
                    br
                    img(src=item.img, class="img-responsive", style="margin: 0 auto")
                    h3: a(href=item.url, target="_blank")=item.name
                    | Libraries: 
                    span=item.lib

